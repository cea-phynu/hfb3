&HFBTHO_FUNCTIONAL
 functional = 'D1S', add_initial_pairing = F, type_of_coulomb = 2 /
&HFBTHO_PAIRING
 user_pairing = F, vpair_n = -300.0, vpair_p = -300.0,
 pairing_cutoff = 60.0, pairing_feature = 0.5 /
&HFBTHO_CONSTRAINTS
 lambda_values = 1, 2, 3, 4, 5, 6, 7, 8,
 lambda_active = 1, 1, 1, 1, 1, 1, 0, 0,
