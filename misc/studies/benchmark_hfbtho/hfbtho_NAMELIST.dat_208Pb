&HFBTHO_GENERAL
 number_of_shells = 16, oscillator_length = 2.223, basis_deformation = 0.0,
 proton_number = 82, neutron_number = 126, type_of_calculation = 1 /
&HFBTHO_INITIAL
 beta2_deformation = 0.0, beta3_deformation = 0.0, beta4_deformation = 0.0 /
&HFBTHO_ITERATIONS
